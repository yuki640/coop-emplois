+/* Réinitialisation des marges et des rembourrages par défaut du navigateur */
+* {
+  margin: 0;
+  padding: 0;
+  box-sizing: border-box;
+}
+
+/* Styles de base pour le corps de la page */
+body {
+  font-family: Arial, sans-serif;
+  background-color: #f0f0f0;
+}
+
+/* Styles pour l'en-tête (barre de navigation en haut) */
+header {
+  background-color: #333;
+  color: #fff;
+  padding: 10px 0;
+}
+
+header nav ul {
+  list-style-type: none;
+  text-align: left;
+}
+
+header nav ul li {
+  display: inline;
+  margin-right: 20px;
+}
+
+header nav ul li:last-child {
+  margin-right: 0;
+}
+
+header nav ul li a {
+  text-decoration: none;
+  color: #fff;
+}
+
+/* Styles pour aligner à gauche et à droite */
+header nav ul li.menu-item.left {
+  float: left;
+}
+
+header nav ul li.menu-item.right {
+  float: right;
+  margin-right: 0; /* Enlever la marge à droite pour le dernier élément aligné à droite */
+}
+
+/* Styles pour le contenu principal */
+main {
+  padding: 20px;
+}
+
+/* Styles pour le pied de page */
+footer {
+  background-color: #333;
+  color: #fff;
+  text-align: center;
+  padding: 10px 0;
+}
